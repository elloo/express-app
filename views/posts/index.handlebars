{{#each posts}}
    <div class="row">
        <div class="col s12 m6">
            <div class="card indigo">
                <div class="card-content white-text">
                    <span class="card-title">{{this.title}}</span>
                    <p>{{this.date}}</p>
                    <p class="flow-text">{{this.content}}</p>
                </div>
            </div>
        </div>
    </div>
{{/each}}